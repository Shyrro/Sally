{
  "name": "svelte-kit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "svelte-kit sync && vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "@sally/s-tag": "workspace:*",
    "@sally/layout": "workspace:*",
    "@sally/theme-utils": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.2.0",
    "unocss": "^0.46.0",
    "@unocss/transformer-directives": "^0.46.2"
  },
  "type": "module"
}
